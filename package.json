  "description": "Node TypeScript authentication / authorization system for mongo DB",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm test && rimraf dist && tsc --project tsconfig.json",
    "test": "set JASMINE_CONFIG_PATH=jasmine.json&& jasmine-ts src/**/*.spec.ts --watch src/**/*.ts",
    "autotest": "nodemon --watch src/**/*.* --exec npm test"
