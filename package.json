    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3",
    "typings": "^1.4.0"
  },
  "dependencies": {
    "aramsay-framework": "^1.0.1",
    "aramsay-injector": "^1.0.1",
    "bcrypt-nodejs": "0.0.3",
    "express": "^4.14.0",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.16.3",
    "moment": "^2.15.1",
    "mongoose": "^4.6.2",
    "passport": "^0.3.2"
